<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>
    <package name="cart" namespace="/cart" extends="struts-default">

        <action name="AddToCart" class="com.baizhi.action.CartAction" method="addToCart">
            <result name="success" type="redirect">/cart/cart_list.jsp</result>
        </action>
        <action name="updateCart" class="com.baizhi.action.CartAction" method="updateCart">
            <result name="success" type="redirect">/cart/cart_list.jsp</result>
        </action>
    </package>

</struts>